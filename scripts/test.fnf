function onCreate()
    luaDebugMode = true
    sprite('test', 'sick', 0, 0)
    spriteCenter('test')
    add('test', true)

    backdrop('logo', 'logo', 'xy');
    setScrollFactor('logo', 0.5, 0.5);
    velocity('logo', 100, 100);
    add('logo');
end

function onBeatHit()
    if curBeat % 4 == 0 then
        scaleObject('logo', 1.1, 1.1)
        velocity('logo', 5, 5);
        doTweenX('logoX', 'logo.velocity', 100, 0.35, 'cubeOut')
        doTweenY('logoY', 'logo.velocity', 100, 0.35, 'cubeOut')
        doTweenX('logoXS', 'logo.scale', 1, 0.45, 'cubeOut')
        doTweenY('logoYS', 'logo.scale', 1, 0.45, 'cubeOut')
    end
end